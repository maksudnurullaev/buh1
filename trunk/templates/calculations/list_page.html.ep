% my $calcs = stash 'calculations' ;
% if( scalar keys %{$calcs} ) {
 <ul>
  % for my $key (reverse sort keys %{$calcs}){
   <li>
    %# admin or user part
    % my $result = ($controller eq 'calculations') ? db_calculate($key) : cdb_calculate($key) ;
    %= $result ? currency_format($result) : '(?)' ;
    %= link_to $calcs->{$key}{description} => ($link_path_prefix . $key) ;
   </li>
  % }
% } else {
 <br /><%= ml 'No data!' %>
% }
