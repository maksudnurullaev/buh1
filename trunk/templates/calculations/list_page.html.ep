% my $calcs = stash 'calculations' ;
% if( scalar keys %{$calcs} ) {
 <ul>
  % for my $key (reverse sort keys %{$calcs}){
   <li>
    % if( $_editable ) {
     %= link_to $calcs->{$key}{description} => "/$_c/$_a_edit/$key"
    % } else {
     %= link_to $calcs->{$key}{description} => "/$_c/$_a_test/$key"
    % }
   </li>
  % }
% } else {
 <br /><%= ml 'No data!' %>
% }
