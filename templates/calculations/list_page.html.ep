% my $calcs = stash 'calculations' ;
% if( scalar keys %{$calcs} ) {
 <ul>
  % for my $key (reverse sort keys %{$calcs}){
   <li>
    %# admin or user part
    % if( $controller eq 'calculations' ) { 
     %= (currency_format db_calculate($key)) || '???' ;
    % } else {
     %= currency_format cdb_calculate($key) || '???' ;
    % }
    %= link_to $calcs->{$key}{description} => ($link_path_prefix . $key) ;
   </li>
  % }
% } else {
 <br /><%= ml 'No data!' %>
% }
