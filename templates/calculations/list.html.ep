% use Utils::Calculations;
% my $calcs = Utils::Calculations::get_client_calcs($self,$payload) ;
% if( scalar keys %{$calcs} ) {
 <ul>
  % for my $key (reverse sort keys %{$calcs}){
   <li>
    %# admin or user part
    % my $result = cdb_calculate($key) || db_calculate($key) ;
    %= $result ? currency_format($result) : '(?)' ;
    %= link_to $calcs->{$key}{description} => $key ;
   </li>
  % }
  </ul>
% } else {
 <br /><%= ml 'No data!' %>
% }
